nlp = spacy.load("en_core_web_sm")

# Annotated data for Scalar Resonance
scalar_resonance_data = [
    ("Scalar Resonance involves self-sustaining oscillations.", {"entities": [(0, 15, "THEORY")]}),
    # Add more annotated examples
]

